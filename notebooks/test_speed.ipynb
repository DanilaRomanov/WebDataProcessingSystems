    "def determine_max_views(x):\n",
    "    # make second column into integer\n",
    "    x[:,1] = x[:,1].astype(int)\n",
    "    # return index of max value\n",
    "    return np.argmax(x[:,1], axis=0)\n",
    "\n",
    "def damerau_levenshtein_distance(s1, s2):\n",
    "  # Initialize the distance matrix\n",
    "  m = len(s1)\n",
    "  n = len(s2)\n",
    "  distance = [[0] * (n + 1) for _ in range(m + 1)]\n",
    "  for i in range(m + 1):\n",
    "    distance[i][0] = i\n",
    "  for j in range(n + 1):\n",
    "    distance[0][j] = j\n",
    "\n",
    "  # Calculate the distance\n",
    "  for i in range(1, m + 1):\n",
    "    for j in range(1, n + 1):\n",
    "      cost = 0 if s1[i - 1] == s2[j - 1] else 1\n",
    "      distance[i][j] = min(distance[i - 1][j] + 1,\n",
    "                           distance[i][j - 1] + 1,\n",
    "                           distance[i - 1][j - 1] + cost)\n",
    "      if i > 1 and j > 1 and s1[i - 1] == s2[j - 2] and s1[i - 2] == s2[j - 1]:\n",
    "        distance[i][j] = min(distance[i][j], distance[i - 2][j - 2] + cost)\n",
    "\n",
    "  # Return the distance\n",
    "  return distance[m][n]\n",
    "\n",
    "def string_similarity(s1, s2):\n",
    "  # Calculate the Levenshtein distance between the two strings\n",
    "  distance = damerau_levenshtein_distance(s1, s2)\n",
    "\n",
    "  # Normalize the distance by the length of the longer string\n",
    "  max_length = max(len(s1), len(s2))\n",
    "  similarity = 1 - (distance / max_length)\n",
    "\n",
    "  # Return the similarity as a percentage\n",
    "  return similarity * 100"
